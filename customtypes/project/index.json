{
  "id": "project",
  "label": "Project",
  "format": "page",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "config": {
          "label": "UID"
        },
        "type": "UID"
      },
      "title": {
        "type": "Text",
        "config": {
          "label": "Title",
          "placeholder": ""
        }
      },
      "is_featured": {
        "type": "Boolean",
        "config": {
          "label": "Is Featured",
          "placeholder_false": "false",
          "placeholder_true": "true",
          "default_value": false
        }
      },
      "tech_stack": {
        "type": "Group",
        "config": {
          "label": "Tech Stack",
          "repeat": true,
          "fields": {
            "name": {
              "type": "Text",
              "config": {
                "label": "Name",
                "placeholder": "e.g. React, Next.js, Python, Django"
              }
            },
            "color": {
              "type": "Color",
              "config": {
                "label": "Color",
                "placeholder": "e.g. #fff"
              }
            }
          }
        }
      },
      "source_code": {
        "type": "Link",
        "config": {
          "label": "Source Code",
          "placeholder": "",
          "select": null
        }
      },
      "view_live": {
        "type": "Link",
        "config": {
          "label": "View Live",
          "placeholder": "",
          "select": null
        }
      },
      "date": {
        "type": "Date",
        "config": {
          "label": "Date",
          "placeholder": ""
        }
      },
      "hover_image": {
        "type": "Image",
        "config": {
          "label": "Hover Image",
          "constraint": {},
          "thumbnails": []
        }
      },
      "key_features": {
        "type": "Group",
        "config": {
          "label": "Key Features",
          "repeat": true,
          "fields": {
            "features": {
              "type": "Text",
              "config": {
                "label": "Features",
                "placeholder": ""
              }
            }
          }
        }
      },
      "challenges": {
        "type": "Group",
        "config": {
          "label": "Challenges",
          "repeat": true,
          "fields": {
            "challenges": {
              "type": "Text",
              "config": {
                "label": "Challenges",
                "placeholder": ""
              }
            }
          }
        }
      },
      "role_contribution": {
        "type": "StructuredText",
        "config": {
          "label": "Role & Contribution ",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph,strong"
        }
      },
      "key_results": {
        "type": "StructuredText",
        "config": {
          "label": "Key Results & Impacts",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph,list-item,o-list-item,embed,strong"
        }
      },
      "development_time": {
        "type": "Text",
        "config": {
          "label": "Development Time",
          "placeholder": "e.g. 5 weeks"
        }
      },
      "key_achievement": {
        "type": "Text",
        "config": {
          "label": "Key Achievement",
          "placeholder": ""
        }
      },
      "current_status": {
        "type": "Select",
        "config": {
          "label": "Current Status",
          "placeholder": "e.g. Active Development, Completed",
          "options": ["Active Development", "Completed"],
          "default_value": "Active Development"
        }
      },
      "accessibility": {
        "type": "Select",
        "config": {
          "label": "Accessibility ",
          "placeholder": "e.g. Open Source project, Private project",
          "options": ["Open Source Project", "Private Project"],
          "default_value": "Open Source Project"
        }
      },
      "started_data": {
        "type": "Text",
        "config": {
          "label": "Started Date",
          "placeholder": "e.g. 2024, 2024"
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "image_block": {
              "type": "SharedSlice"
            },
            "text_block": {
              "type": "SharedSlice"
            }
          }
        }
      }
    },
    "SEO & Metadata": {
      "meta_description": {
        "config": {
          "label": "Meta Description",
          "placeholder": "A brief summary of the page"
        },
        "type": "Text"
      },
      "meta_image": {
        "config": {
          "constraint": {
            "height": 1260,
            "width": 2400
          },
          "label": "Meta Image",
          "thumbnails": []
        },
        "type": "Image"
      },
      "meta_title": {
        "config": {
          "label": "Meta Title",
          "placeholder": "A title of the page used for social media and search engines"
        },
        "type": "Text"
      }
    }
  }
}
